{
  "name": "davidgarvie.co.uk",
  "version": "0.1.0",
  "description": "The personal website of David Garvie",
  "main": "index.js",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "release": "standard-version",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "blog",
    "gatsby"
  ],
  "author": "David Garvie",
  "license": "ISC",
  "dependencies": {
    "date-fns": "^2.13.0",
    "dotenv": "^8.2.0",
    "gatsby": "^2.21.24",
    "gatsby-source-contentful": "^2.3.3",
    "gatsby-transformer-remark": "^2.8.7",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "commitizen": "^4.1.2",
    "cz-conventional-changelog-emoji": "^0.1.0",
    "husky": "^4.2.5",
    "standard-version": "^8.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-emoji"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  }
}
